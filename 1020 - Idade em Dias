#include <stdio.h>
 
int main() {
 
    int total,anos,meses,dias;
    
    scanf("%i",&total);
    
    if(total > 365){
    anos = total / 365;
    total = total - 365*anos;
    }
    if(total >= 30){
    meses = total / 30;
    total = total - 30*meses;
    }
    dias = total;
    
    printf("%i ano(s)\n",anos);
    printf("%i mes(es)\n",meses);
    printf("%i dia(s)\n",dias);
 
    return 0;
}
